{
  "name": "yandex-middle-dev",
  "version": "1.0.0",
  "description": "online six month course in yandex practicum",
  "scripts": {
    "start": "node src/server.js",
    "build": "parcel build src/index.html src/templates/pages/**/**.html --no-optimize",
    "lint": "eslint src/**/**/*.ts src/*.js --fix",
    "stylelint": "stylelint --fix \"src/sass/**/*.scss\" --custom-syntax postcss-scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kan88/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [
    "JS",
    "Handlebars",
    "Postcss"
  ],
  "author": "Evgeny Kan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kan88/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/kan88/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {

    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "stylelint": "14.14.0",
    "stylelint-config-htmlacademy": "1.1.2",
    "typescript": "4.8.4",
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@parcel/transformer-sass": "2.7.0",
    "postcss-scss": "4.0.5",
    "parcel": "2.7.0"
  },
  "dependencies": {
    "handlebars": "4.7.7",
    "uuid": "9.0.0",
    "npm-path": "2.0.4",
    "express": "4.18.2"
  },
  "engines": {
    "node": ">=14.0"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": ">= 50%"
      }
    }
  }
}
